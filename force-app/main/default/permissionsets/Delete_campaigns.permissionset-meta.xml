<?xml version="1.0" encoding="UTF-8"?>
<PermissionSet xmlns="http://soap.sforce.com/2006/04/metadata">
    <description>Ability to delete fundraising campaigns - for removing testing campaigns from 3rd party apps. Eg Funraisin</description>
    <hasActivationRequired>false</hasActivationRequired>
    <label>Delete Fundraising campaigns</label>
    <objectPermissions>
        <allowCreate>true</allowCreate>
        <allowDelete>true</allowDelete>
        <allowEdit>true</allowEdit>
        <allowRead>true</allowRead>
        <modifyAllRecords>false</modifyAllRecords>
        <object>Campaign</object>
        <viewAllRecords>false</viewAllRecords>
    </objectPermissions>
    <recordTypeVisibilities>
        <recordType>Campaign.Fundraising</recordType>
        <visible>true</visible>
    </recordTypeVisibilities>
</PermissionSet>
