<?xml version="1.0" encoding="UTF-8"?>
<ApprovalProcess xmlns="http://soap.sforce.com/2006/04/metadata">
    <active>true</active>
    <allowRecall>true</allowRecall>
    <allowedSubmitters>
        <submitter>System_Administrator</submitter>
        <type>roleSubordinatesInternal</type>
    </allowedSubmitters>
    <approvalPageFields>
        <field>Granted_Authority_Summary__c</field>
        <field>Name</field>
        <field>Application_Details__c</field>
        <field>Event_Fundraising_Ratio__c</field>
        <field>Event_Meets_60_40_rule__c</field>
        <field>Comments_in_support_of_application__c</field>
        <field>Application_Made_By__c</field>
        <field>Branch__c</field>
        <field>ATF_Status__c</field>
        <field>Budgeted_Net_Income__c</field>
        <field>Campaign__c</field>
        <field>Contact__c</field>
        <field>CreatedBy</field>
        <field>Event_Attendees__c</field>
        <field>Event_Date__c</field>
        <field>Event_Expenses__c</field>
        <field>Event_Income_Per_Participant__c</field>
        <field>Event_ticket_price__c</field>
        <field>Expense_Amount__c</field>
        <field>Expense_at_your_event__c</field>
        <field>Explained_online_fundraising__c</field>
        <field>Extra_support_required__c</field>
        <field>Fundraiser_includes_event__c</field>
        <field>Fundraiser_Page_URL__c</field>
        <field>Fundraising_End_Date__c</field>
        <field>Fundraising_Mechanisms__c</field>
        <field>Fundraising_Mechanisms_Comment__c</field>
        <field>Fundraising_on_behalf_of_an_Organisation__c</field>
        <field>Fundraising_Start_Date__c</field>
        <field>Gross_Income__c</field>
        <field>High_Risk_Links__c</field>
        <field>Insurance_Coverage__c</field>
        <field>Internal_Research__c</field>
        <field>LastModifiedBy</field>
        <field>Local_media_opportunity__c</field>
        <field>Motivation_for_Participating__c</field>
        <field>Organisation_ABN__c</field>
        <field>Organisation_Address__c</field>
        <field>Organisation_Name__c</field>
        <field>Page_ID__c</field>
        <field>Position_at_Organisation__c</field>
        <field>PSP_Booking__c</field>
        <field>Questions_from_call__c</field>
        <field>Raffle_permit_application__c</field>
        <field>Raffle_permit_required__c</field>
        <field>Raffle_print_tickets__c</field>
        <field>Raffle_prize_pool__c</field>
        <field>Raffle_ticket_price__c</field>
        <field>Raffle_tickets_sold_before_event__c</field>
        <field>Risks_associated_with_the_fundraiser__c</field>
        <field>Running_Raffle__c</field>
        <field>SITC_Fundraising__c</field>
        <field>Target__c</field>
        <field>Use_Of_Fundraising_Logo__c</field>
        <field>Venue__c</field>
        <field>Why_are_they_supporting_CQ__c</field>
    </approvalPageFields>
    <approvalStep>
        <allowDelegate>true</allowDelegate>
        <approvalActions>
            <action>
                <name>New_ATF_Approval_Request</name>
                <type>Alert</type>
            </action>
        </approvalActions>
        <assignedApprover>
            <approver>
                <name>Approver__c</name>
                <type>relatedUserField</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <label>Approval Step 1</label>
        <name>Approval_Step_1</name>
    </approvalStep>
    <enableMobileDeviceAccess>false</enableMobileDeviceAccess>
    <entryCriteria>
        <criteriaItems>
            <field>Request_To_Fundraise__c.ATF_Status__c</field>
            <operation>equals</operation>
            <value>Request Approval</value>
        </criteriaItems>
    </entryCriteria>
    <finalApprovalActions>
        <action>
            <name>Approve_Fundraiser</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>Send_RTF_approval_email</name>
            <type>Alert</type>
        </action>
    </finalApprovalActions>
    <finalApprovalRecordLock>false</finalApprovalRecordLock>
    <finalRejectionActions>
        <action>
            <name>Decline_Fundraiser</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>Notify_RTF_Rejection</name>
            <type>Alert</type>
        </action>
    </finalRejectionActions>
    <finalRejectionRecordLock>false</finalRejectionRecordLock>
    <initialSubmissionActions>
        <action>
            <name>New_ATF_Approval_Request</name>
            <type>Alert</type>
        </action>
    </initialSubmissionActions>
    <label>ATF Approval</label>
    <nextAutomatedApprover>
        <useApproverFieldOfRecordOwner>false</useApproverFieldOfRecordOwner>
        <userHierarchyField>Manager</userHierarchyField>
    </nextAutomatedApprover>
    <processOrder>1</processOrder>
    <recordEditability>AdminOrCurrentApprover</recordEditability>
    <showApprovalHistory>true</showApprovalHistory>
</ApprovalProcess>
