<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>59.0</apiVersion>
    <environments>Default</environments>
    <interviewLabel>Update Vol Attendances for {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Update Vol Attendances for leaving volunteers v1</label>
    <processMetadataValues>
        <name>BuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>CanvasMode</name>
        <value>
            <stringValue>AUTO_LAYOUT_CANVAS</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>OriginBuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processType>AutoLaunchedFlow</processType>
    <recordUpdates>
        <name>Find_Vollunteer_Attendances</name>
        <label>Find Volunteer Attendances</label>
        <locationX>176</locationX>
        <locationY>323</locationY>
        <filterLogic>1 AND (2 OR 3 OR 4)</filterLogic>
        <filters>
            <field>EventStartDate__c</field>
            <operator>GreaterThanOrEqualTo</operator>
            <value>
                <elementReference>$Record.Date_To__c</elementReference>
            </value>
        </filters>
        <filters>
            <field>Attendance__c</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>Invited</stringValue>
            </value>
        </filters>
        <filters>
            <field>Attendance__c</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>Accepted</stringValue>
            </value>
        </filters>
        <filters>
            <field>Attendance__c</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>Request to Attend</stringValue>
            </value>
        </filters>
        <inputAssignments>
            <field>Attendance__c</field>
            <value>
                <stringValue>Declined</stringValue>
            </value>
        </inputAssignments>
        <inputReference>$Record.Volunteer_Attendances__r</inputReference>
    </recordUpdates>
    <start>
        <locationX>50</locationX>
        <locationY>0</locationY>
        <connector>
            <targetReference>Find_Vollunteer_Attendances</targetReference>
        </connector>
        <filterLogic>and</filterLogic>
        <filters>
            <field>Date_To__c</field>
            <operator>GreaterThan</operator>
            <value>
                <dateValue>2023-12-31</dateValue>
            </value>
        </filters>
        <filters>
            <field>Volunteer_Status__c</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>Volunteer</stringValue>
            </value>
        </filters>
        <object>Contact</object>
        <recordTriggerType>Update</recordTriggerType>
        <triggerType>RecordAfterSave</triggerType>
    </start>
    <status>Obsolete</status>
</Flow>
