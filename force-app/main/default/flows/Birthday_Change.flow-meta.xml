<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>61.0</apiVersion>
    <description>Migrated from the: Birthday Change workflow rule
Workflow rule description: In case you have to change the date of a birthday&quot;</description>
    <label>Birthday Change</label>
    <migratedFromWorkflowRuleName>Birthday Change</migratedFromWorkflowRuleName>
    <processMetadataValues>
        <name>CanvasMode</name>
        <value>
            <stringValue>AUTO_LAYOUT_CANVAS</stringValue>
        </value>
    </processMetadataValues>
    <processType>AutoLaunchedFlow</processType>
    <recordUpdates>
        <description>Checks the rest birthday box required in workflow for volunteer email</description>
        <name>mainUpdate</name>
        <label>mainUpdate</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <inputAssignments>
            <field>Reset_Birthday_Email_System__c</field>
            <value>
                <booleanValue>true</booleanValue>
            </value>
        </inputAssignments>
        <inputReference>$Record</inputReference>
    </recordUpdates>
    <start>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <connector>
            <targetReference>mainUpdate</targetReference>
        </connector>
        <filterFormula>(MONTH({!$Record.Birthdate})&lt;&gt;MONTH({!$Record.Next_Birthday__c})||DAY({!$Record.Birthdate})&lt;&gt;DAY({!$Record.Next_Birthday__c}))&amp;&amp; {!$Record.Receive_Birthday_Email__c} =TRUE</filterFormula>
        <object>Contact</object>
        <recordTriggerType>CreateAndUpdate</recordTriggerType>
        <triggerType>RecordBeforeSave</triggerType>
    </start>
    <status>Draft</status>
</Flow>
