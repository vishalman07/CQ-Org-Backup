<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Campaign_Type_Validation</fullName>
    <active>false</active>
    <description>Please choose a campaign type</description>
    <errorConditionFormula>AND(
  ISPICKVAL(Funraisin__Campaign_Type__c, &apos;&apos;), 
  ISBLANK(ParentId)
)</errorConditionFormula>
