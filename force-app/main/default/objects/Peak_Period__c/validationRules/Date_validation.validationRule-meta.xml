<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Date_validation</fullName>
    <active>true</active>
    <description>Start date to be before end date</description>
    <errorConditionFormula>Start_Date__c &gt;=  End_Date__c</errorConditionFormula>
    <errorDisplayField>Start_Date__c</errorDisplayField>
    <errorMessage>Start date should be before end date</errorMessage>
</ValidationRule>
